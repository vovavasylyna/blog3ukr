:root{font-family:inter,sans-serif}@supports(font-variation-settings:normal){:root{font-family:inter var,sans-serif}}body{font:1rem/1.5 inter,system-ui,sans-serif;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale;padding:0;margin:0;color:#333;letter-spacing:-.02em}h1,h2,h3,h4,h5,b,strong{font-size:inherit;font-weight:500}h2,h3,h4,h5{margin-top:0}h2 a,h3 a,h4 a,h5 a{text-decoration:none}p:last-child{margin-bottom:.5rem}hr{background:#e6e6e6;height:1px;border:none;max-width:60ch;padding:0;margin:0 auto}header,article,footer{max-width:60ch;margin:0 auto;padding:2rem}article{padding-top:4rem;padding-bottom:1rem}article p a{word-break:break-all}.projects img{background:#e6e6e6}.portfolio img{padding-bottom:1rem;height:auto}.portfolio img img:last-child{padding-bottom:0}footer a{padding-right:.5rem}footer{padding-top:1rem}pre,code{font-size:inherit;background:#f7f7f7}pre{margin-bottom:1.5rem}pre code{display:block;padding:1.5rem;overflow-x:auto}code{background:#f7f7f7;word-break:break-all}.list li{list-style-type:none;margin-bottom:.5rem;display:flex;align-items:baseline}.home p:first-child{margin-top:0}.list,.home ol{padding:0;margin:0}time{font-variant-numeric:tabular-nums;padding-right:1rem;white-space:nowrap}blockquote{margin:1em 0;padding:0 2em;border-left:3px solid #e6e6e6}img,figure{max-width:100%;margin:0}a,a:visited{color:#333;text-underline-offset:.25em;text-decoration-color:#e6e6e6;text-decoration-thickness:1px}.terms{column-count:2}